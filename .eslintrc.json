{
  "plugins": ["react"],
  "extends": [
    "plugin:react/recommended"
  ],
  "rules": {
    "react/prop-types": "off",
    "react/display-name": "off",
    "react/jsx-key": "off"
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}